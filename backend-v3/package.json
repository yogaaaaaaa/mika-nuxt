{
  "name": "backend-v3",
  "version": "3.14.0",
  "private": true,
  "scripts": {
    "preinstall": "command -v link-module-alias && link-module-alias clean || true",
    "postinstall": "link-module-alias",
    "standard": "standard --fix",
    "db-recreate": "sequelize db:drop --config configs/dbConfig.js && sequelize db:create --config configs/dbConfig.js",
    "db-recreate-migrate": "sequelize db:drop --config configs/dbConfig.js && sequelize db:create --config configs/dbConfig.js && sequelize db:migrate --config configs/dbConfig.js",
    "db-drop": "sequelize db:drop --config configs/dbConfig.js",
    "db-create": "sequelize db:create --config configs/dbConfig.js",
    "db-migrate": "sequelize db:migrate --config configs/dbConfig.js",
    "db-migrate-undo": "sequelize db:migrate:undo --config configs/dbConfig.js",
    "db-seed-development": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/development",
    "db-seed-production": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/production",
    "db-seed-card-iin": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/cardIin",
    "db-seed-transaction": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/transaction",
    "test-integration": "mocha --exit --bail ./tests/integration/**/*.test.js",
    "test-integration-vagrant": "MIKA_CONFIG_GROUP=vagrant mocha --exit --bail ./tests/integration/**/*.test.js",
    "test-template": "mocha ./tests/template.test.js",
    "dev-vagrant": "MIKA_CONFIG_GROUP=vagrant node ./apps/core.js"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "base-x": "^3.0.8",
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.1.1",
    "bcrypt": "^4.0.1",
    "ber-tlv": "^0.9.2",
    "ber-tlv-annotations": "^0.9.6",
    "bignumber.js": "^9.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "csv-parse": "^4.9.1",
    "debug": "^4.1.1",
    "digital-signature": "^1.0.4",
    "dtimer": "^0.3.3",
    "dukpt": "^3.0.0",
    "elasticsearch": "^16.7.1",
    "eventemitter2": "^6.4.0",
    "exceljs": "^3.9.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.4.1",
    "fastest-validator": "^1.4.1",
    "handlebars": "^4.7.6",
    "id128": "^1.5.0",
    "ioredis": "^4.16.3",
    "json-diff": "^0.5.4",
    "jsonwebtoken": "^8.5.1",
    "ksuid": "^1.2.0",
    "link-module-alias": "^1.2.0",
    "lodash": "^4.17.15",
    "micromatch": "^4.0.2",
    "moleculer": "^0.14.6",
    "moleculer-repl": "^0.6.4",
    "moment": "^2.25.3",
    "moment-range": "^4.0.2",
    "morgan": "^1.10.0",
    "mqtt": "^4.0.1",
    "multer": "^1.4.2",
    "node-html-parser": "^1.2.16",
    "nodemailer": "^6.4.6",
    "password-validator": "^5.0.3",
    "pg": "^8.2.0",
    "pg-hstore": "^2.3.3",
    "puppeteer": "^3.0.4",
    "redis": "^3.0.2",
    "redlock": "^4.1.0",
    "sequelize": "^5.21.8",
    "strict-password-generator": "^1.1.2",
    "superagent": "^5.2.2",
    "tamper": "^1.1.0",
    "terminal-kit": "^1.35.2",
    "uuid": "^8.0.0",
    "validator": "^13.0.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^7.1.2",
    "sequelize-cli": "^5.5.1",
    "standard": "^14.3.4",
    "supertest": "^4.0.2"
  },
  "_moduleAliases": {
    "~root": ".",
    "apps": "apps",
    "libs": "libs",
    "configs": "configs",
    "models": "models",
    "controllers": "controllers",
    "middlewares": "middlewares",
    "validators": "validators",
    "services": "services",
    "routes": "routes",
    "shells": "shells"
  }
}
