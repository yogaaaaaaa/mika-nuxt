{
  "name": "backend-v3",
  "version": "3.11.1",
  "private": true,
  "scripts": {
    "preinstall": "command -v link-module-alias && link-module-alias clean || true",
    "postinstall": "link-module-alias",
    "standard": "standard --fix",
    "db-recreate": "sequelize db:drop --config configs/dbConfig.js && sequelize db:create --config configs/dbConfig.js",
    "db-recreate-migrate": "sequelize db:drop --config configs/dbConfig.js && sequelize db:create --config configs/dbConfig.js && sequelize db:migrate --config configs/dbConfig.js",
    "db-drop": "sequelize db:drop --config configs/dbConfig.js",
    "db-create": "sequelize db:create --config configs/dbConfig.js",
    "db-migrate": "sequelize db:migrate --config configs/dbConfig.js",
    "db-migrate-undo": "sequelize db:migrate:undo --config configs/dbConfig.js",
    "db-seed-development": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/development",
    "db-seed-production": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/production",
    "db-seed-card-iin": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/cardIin",
    "db-seed-transaction": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/transaction",
    "test-unit": "mocha --reporter spec ./tests/unit/*.js",
    "test-integration": "mocha --reporter list ./tests/integration/*.js",
    "test-template": "mocha --reporter list ./tests/template.test.js"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "base-x": "^3.0.7",
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.1.1",
    "base64-img": "^1.0.4",
    "bcrypt": "^3.0.6",
    "ber-tlv": "^0.9.2",
    "ber-tlv-annotations": "^0.9.6",
    "bignumber.js": "^9.0.0",
    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "csv-parse": "^4.6.5",
    "dtimer": "^0.3.3",
    "dukpt": "^3.0.0",
    "elasticsearch": "^16.6.0",
    "eventemitter2": "^5.0.1",
    "exceljs": "^3.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.2.0",
    "fastest-validator": "^0.6.18",
    "handlebars": "^4.5.3",
    "id128": "^1.4.0",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "ksuid": "^1.2.0",
    "lodash": "^4.17.15",
    "micromatch": "^4.0.2",
    "moleculer": "^0.13.11",
    "moleculer-repl": "^0.6.1",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "morgan": "^1.9.1",
    "mqtt": "^3.0.0",
    "multer": "^1.4.2",
    "node-banner": "^1.3.2",
    "node-html-parser": "^1.1.16",
    "nodemailer": "^6.3.1",
    "password-validator": "^5.0.2",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "puppeteer": "^1.20.0",
    "redis": "^2.8.0",
    "redlock": "^4.1.0",
    "sequelize": "^5.21.1",
    "strict-password-generator": "^1.1.2",
    "superagent": "^5.1.0",
    "tamper": "^1.1.0",
    "terminal-kit": "^1.32.3",
    "uuid": "^3.3.3",
    "validator": "^11.1.0",
    "xml-js": "^1.6.11",
    "debug": "^4.1.1",
    "link-module-alias": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.2",
    "sequelize-cli": "^5.5.1",
    "standard": "^14.3.1",
    "supertest": "^4.0.2"
  },
  "_moduleAliases": {
    "~root": ".",
    "libs": "libs",
    "configs": "configs",
    "models": "models",
    "controllers": "controllers",
    "middlewares": "middlewares",
    "validators": "validators",
    "services": "services",
    "routes": "routes",
    "shells": "shells"
  }
}
