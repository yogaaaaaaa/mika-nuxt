{
  "name": "backend-v3",
  "version": "3.0.1",
  "private": true,
  "scripts": {
    "db-recreate": "sequelize db:drop --config configs/dbConfig.js && sequelize db:create --config configs/dbConfig.js",
    "db-drop": "sequelize db:drop --config configs/dbConfig.js",
    "db-create": "sequelize db:create --config configs/dbConfig.js",
    "db-migrate": "sequelize db:migrate --config configs/dbConfig.js",
    "db-migrate-undo": "sequelize db:migrate:undo --config configs/dbConfig.js",
    "db-seed-development": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/development",
    "db-seed-production": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/production",
    "test-unit": "mocha --reporter spec ./tests/unit/*.js",
    "test-integration": "mocha --reporter list ./tests/integration/*.js",
    "test-template": "mocha --reporter list ./tests/template.test.js"
  },
  "dependencies": {
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.1.1",
    "base64-img": "^1.0.4",
    "bcrypt": "^3.0.6",
    "ber-tlv": "^0.9.2",
    "ber-tlv-annotations": "^0.9.6",
    "bigint-buffer": "^1.1.2",
    "bluebird": "^3.5.5",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "currency-formatter": "^1.5.4",
    "currency.js": "^1.2.2",
    "dtimer": "^0.3.3",
    "dukpt": "^2.1.0",
    "exceljs": "^1.13.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.0.0",
    "express-validator": "^6.1.1",
    "handlebars": "^4.1.2",
    "ioredis": "^4.11.1",
    "jsonwebtoken": "^8.4.0",
    "ksuid": "^1.2.0",
    "lodash": "^4.17.11",
    "mariadb": "^2.0.5",
    "micromatch": "^4.0.2",
    "moleculer": "^0.13.9",
    "moleculer-repl": "^0.5.7",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "morgan": "^1.9.1",
    "mqtt": "^2.18.8",
    "multer": "^1.3.1",
    "mysql2": "^1.6.4",
    "nodemailer": "^6.2.1",
    "numeral": "^2.0.6",
    "puppeteer": "^1.18.1",
    "redis": "^2.8.0",
    "sequelize": "^5.9.4",
    "superagent": "^5.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "debug": "^4.1.1",
    "mocha": "^6.1.4",
    "sequelize-cli": "^5.5.0",
    "supertest": "^4.0.2"
  }
}
