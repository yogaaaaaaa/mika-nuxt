{
  "name": "backend-v3",
  "version": "3.10.0",
  "private": true,
  "scripts": {
    "standard": "standard --fix",
    "db-recreate": "sequelize db:drop --config configs/dbConfig.js && sequelize db:create --config configs/dbConfig.js",
    "db-drop": "sequelize db:drop --config configs/dbConfig.js",
    "db-create": "sequelize db:create --config configs/dbConfig.js",
    "db-migrate": "sequelize db:migrate --config configs/dbConfig.js",
    "db-migrate-undo": "sequelize db:migrate:undo --config configs/dbConfig.js",
    "db-seed-development": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/development",
    "db-seed-production": "sequelize db:seed:all --config configs/dbConfig.js --seeders-path seeders/production",
    "test-unit": "mocha --reporter spec ./tests/unit/*.js",
    "test-integration": "mocha --reporter list ./tests/integration/*.js",
    "test-template": "mocha --reporter list ./tests/template.test.js"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "base-x": "^3.0.7",
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.1.1",
    "base64-img": "^1.0.4",
    "bcrypt": "^3.0.6",
    "ber-tlv": "^0.9.2",
    "ber-tlv-annotations": "^0.9.6",
    "bignumber.js": "^9.0.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dtimer": "^0.3.3",
    "dukpt": "^3.0.0",
    "eventemitter2": "^5.0.1",
    "exceljs": "^1.15.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.2.0",
    "fastest-validator": "^0.6.17",
    "handlebars": "^4.2.0",
    "id128": "^1.4.0",
    "ioredis": "^4.14.0",
    "jsonwebtoken": "^8.5.1",
    "ksuid": "^1.2.0",
    "lodash": "^4.17.15",
    "mariadb": "^2.1.1",
    "micromatch": "^4.0.2",
    "moleculer": "^0.13.10",
    "moleculer-repl": "^0.6.1",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "morgan": "^1.9.1",
    "mqtt": "^2.18.8",
    "multer": "^1.4.2",
    "mysql2": "^1.7.0",
    "node-html-parser": "^1.1.16",
    "nodemailer": "^6.3.0",
    "password-validator": "^4.1.2",
    "puppeteer": "^1.19.0",
    "redis": "^2.8.0",
    "sequelize": "^5.18.4",
    "strict-password-generator": "^1.1.2",
    "superagent": "^5.1.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "standard": "^14.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "debug": "^4.1.1",
    "mocha": "^6.2.0",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  }
}
